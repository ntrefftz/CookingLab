<h1>Ingredientes</h1>
<a href="/recetas/ingredienteInd/aniadir"><button>Añadir ingrediente</button></a>
<div id="catalogo" class="cards_container">
    <% ingredientes.forEach(ingredienteInd => { %>
        <article class="card">
            <img src="https://mdn.github.io/shared-assets/images/examples/balloons.jpg" alt="Pronto habrá una foto"  />
            <header>
                <a href="ingredienteInd/?id=<%= ingredienteInd.id %>">
                    <h4><%= ingredienteInd.nombre %></h4>
                </a>
            </header>
        </article>
        <% }) %>
</div>



